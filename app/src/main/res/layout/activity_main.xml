<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">


    <LinearLayout
        android:id="@+id/activity_mainLinearLayout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:orientation="vertical">


        <LinearLayout
            android:id="@+id/estadolatamsrc2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/eresviplatamsrc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal">


                <include
                    layout="@layout/zacdevz2"/>

            </LinearLayout>


            <LinearLayout
                android:id="@+id/erespobrelatamsrc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal">

                <include
                    layout="@layout/zacdevz"/>


            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:id="@+id/tunnelCardViewgatesccn"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            
         

            <androidx.cardview.widget.CardView
                android:id="@+id/connection_cardView"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5.0dip"
                app:cardBackgroundColor="@color/card_view_bg"
                app:cardCornerRadius="2.0dip">

                
                
                			<ImageView
				android:layout_gravity="bottom"
				android:background="@drawable/button_blue"
				android:layout_width="fill_parent"
				android:layout_height="5.0dip"/>
                
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="10.0dip">

                    <TextView
                        android:id="@+id/payload"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"

                        android:hint="@string/payload" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/payloadEdit"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/activity_starterButtonMain"
                        android:layout_alignParentStart="true"
                        android:scrollbars="vertical"
                        app:drawableTint="@color/white" />


                    
                    <TextView
                        android:id="@+id/inputLayoutPayloadDefault"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/payload"
                        android:visibility="gone" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/activity_starterButtonMain"
                        android:layout_alignParentStart="true"
                        android:enabled="false"
                        android:visibility="gone"
                        android:text="@string/default_payload" />


                    <LinearLayout
                        android:id="@+id/proxyLayout"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10.0dip"
                        android:baselineAligned="false"
                        android:foreground="?android:selectableItemBackground"
                        android:orientation="horizontal"
                        android:weightSum="1.0">

                        <LinearLayout
                            android:layout_width="0.0dip"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.9"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:maxLines="1"
                                android:paddingStart="5.0dip"
                                android:paddingEnd="5.0dip"
                                android:text="@string/remote_proxy" />

                            <TextView
                                android:id="@+id/proxyText"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:maxLines="1"
                                android:paddingStart="5.0dip"
                                android:paddingEnd="5.0dip"
                                android:text="@string/censored_string" />

                        </LinearLayout>

                        <ImageView
                            android:id="@+id/imageViewProxy"
                            android:layout_width="0.0dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_weight="0.1"
                            android:src="@drawable/ic_editor_mode_edit"
                            app:tint="#048393" />

                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/sslLayout"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5.0dip"
                        android:baselineAligned="false"
                        android:foreground="?android:selectableItemBackground"
                        android:orientation="horizontal"
                        android:weightSum="1.0">

                        <LinearLayout
                            android:layout_width="0.0dip"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.9"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/textViewSniTitle"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:maxLines="1"
                                android:paddingStart="5.0dip"
                                android:paddingEnd="5.0dip"
                                android:text="@string/sni_title"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/sslText"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:maxLines="1"
                                android:paddingStart="5.0dip"
                                android:paddingEnd="5.0dip"
                                android:text="@string/censored_string" />

                        </LinearLayout>

                        <ImageView
                            android:id="@+id/imageViewSNI"
                            android:layout_width="0.0dip"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_weight="0.1"
                            android:src="@drawable/ic_editor_mode_edit"
                            app:tint="#048697" />

                    </LinearLayout>

                </LinearLayout>

            </androidx.cardview.widget.CardView>
            
            
            <androidx.cardview.widget.CardView
                android:id="@+id/tunnelCardView"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5.0dip"
                android:foreground="?android:selectableItemBackground"
                android:paddingTop="10.0dip"
                app:cardBackgroundColor="@color/card_view_bg"
                app:cardCornerRadius="2.0dip">

                
                
                			<ImageView
				android:layout_gravity="bottom"
				android:background="@drawable/button_blue"
				android:layout_width="fill_parent"
				android:layout_height="5.0dip"/>
                
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="10.0dip">

                    <TextView
                        android:id="@+id/activitymainTextView1"
                        android:layout_width="0.0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.0"
                        android:drawablePadding="5.0dip"
                        android:ellipsize="end"
                        android:gravity="center_vertical"
                        android:maxLines="1"
                        android:padding="5.0dip"
                        android:textAppearance="@style/TextAppearance.AppCompat.Small"
                        android:tint="@color/white"
                        app:drawableLeftCompat="@drawable/ic_action_matrix"
                        app:drawableStartCompat="@drawable/ic_action_matrix"
                        app:drawableTint="@color/white" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="end"
                        android:src="@drawable/ic_navigation_chevron_right"
                        app:tint="@color/white" />

                </LinearLayout>

            </androidx.cardview.widget.CardView>
        </LinearLayout>

      
        
          
               
            
        
        <androidx.cardview.widget.CardView
            android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginTop="0.0dip"
                    android:layout_marginRight="10.0dip"
                    android:layout_marginBottom="10.0dip"
                    
                    ads:cardCornerRadius="10.0dip"
                    ads:cardElevation="10.0dip">

            <LinearLayout
                android:id="@+id/estadolatamsrc1"
                android:layout_height="63.0dip"
                android:layout_width="fill_parent"
                android:gravity="center"
                android:orientation="vertical">

              
                    <LinearLayout
                        android:background="@drawable/button"
                        android:id="@+id/layout_mainservers"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content">

                        <include
                            layout="@layout/servers_item2" />

                    </LinearLayout>

                   

                </LinearLayout>

        </androidx.cardview.widget.CardView>
        
          <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/activity_starterButtonMain"
            android:layout_width="fill_parent"
            android:layout_height="42dp"
            android:layout_marginLeft="5.0dip"
            android:layout_marginTop="15.0dip"
            android:layout_marginRight="5.0dip"
            android:layout_marginBottom="5.0dip"
            android:background="@drawable/button"
            android:longClickable="false"
            android:text="@string/start"
            android:textColor="@color/white" />
        
        

            


       


        


        <androidx.cardview.widget.CardView
            android:id="@+id/cardViewNews"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
           android:visibility="gone"
            android:layout_margin="5.0dip"
            android:foreground="?android:selectableItemBackground"
            android:onClick="hype"
            android:paddingTop="10.0dip"
            app:cardBackgroundColor="@color/card_view_bg"
            app:cardCornerRadius="2.0dip">

            <LinearLayout
                android:layout_width="wrap_content"
                android:visibility="gone"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/newsTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:autoLink="web"
                    android:drawablePadding="5.0dip"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:visibility="gone"
                    android:linksClickable="true"
                    android:maxLines="1"
                    android:padding="5.0dip"
                    android:text="Canal Telegram"
                    android:textAppearance="@style/TextAppearance.AppCompat.Small"
                    android:tint="@color/white"
                    app:drawableLeftCompat="@drawable/ic_action_newspaper"
                    app:drawableStartCompat="@drawable/ic_action_newspaper" />

            </LinearLayout>

        </androidx.cardview.widget.CardView>


        <FrameLayout
            android:id="@+id/admobAdViewContainer"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="10.0dip">

        </FrameLayout>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardViewAds"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8.0dip"
            android:visibility="gone"
            app:cardBackgroundColor="@color/card_view_bg"
            app:cardCornerRadius="2.0dip">

            <FrameLayout
                android:id="@+id/admob_native_ad_container"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

            </FrameLayout>


        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/activitymainCardView1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5.0dip"
            android:layout_marginBottom="10.0dip"
            android:paddingTop="10.0dip"
            android:visibility="gone"
            app:cardBackgroundColor="@color/card_view_bg"
            app:cardCornerRadius="2.0dip">

            <TextView
                android:id="@+id/activity_mainMensagemConfigTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:autoLink="web"
                android:linksClickable="true"
                android:padding="5.0dip"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textIsSelectable="true" />

        </androidx.cardview.widget.CardView>


    </LinearLayout>


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="@color/container"
        android:src="@drawable/ic_info"
        android:visibility="gone"
        app:drawableTint="@color/fabColor"
        app:tint="@color/fabColor" />

    <com.google.android.gms.ads.AdView xmlns:ads="http://schemas.android.com/apk/res-auto"
        android:id="@+id/bannerhome"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center"
        android:layout_marginTop="8dp"
        ads:adSize="BANNER"
        ads:adUnitId="@string/admobBannerads" />

</RelativeLayout>

